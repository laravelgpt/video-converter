{
    "name": "multipay/multipay-php",
    "description": "A multi-gateway payment processing library for PHP, providing a consistent API for BinancePay, bKash, Nagad, Upay, STC Pay, Urpay, Barq, PayPal, PhonePe, Paytm, Razorpay, MobiKwik, MobilyPay, Easypaisa, Jazzcash, Omantel, BankDhofar, American Express, GooglePay, Stripe, Adyen, TwoCheckout, Wise, Braintree, AmazonPay, Mpesa, Flutterwave, Paystack, PayFast, PeachPayments, DpoPay, Interswitch, Alipay, WeChatPay, UnionPay, BillDesk, AmarPay, SSLCommerz, and PortWallet.",
    "type": "library",
    "keywords": ["payment", "gateway", "binance", "bkash", "nagad", "upay", "stcpay", "urpay", "barq", "paypal", "phonepe", "paytm", "razorpay", "mobikwik", "mobilypay", "easypaisa", "jazzcash", "omantel", "bankdhofar", "amex", "googlepay", "stripe", "adyen", "twocheckout", "wise", "braintree", "amazonpay", "mpesa", "flutterwave", "paystack", "payfast", "peachpayments", "dpopay", "interswitch", "alipay", "wechatpay", "unionpay", "billdesk", "amarpay", "sslcommerz", "portwallet", "php", "laravel"],
    "homepage": "https://github.com/laravelgpt/multipay-php",
    "license": "MIT",
    "authors": [
        {
            "name": "shamim / Organization",
            "email": "rdxunlock@gmail.com",
            "homepage": "https://shamimhossain.com.bd",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "For a more robust HTTP client implementation (highly recommended for production)."
    },
    "autoload": {
        "psr-4": {
            "Multipay\\Payment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Multipay\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
